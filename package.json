{
  "name": "desplega",
  "version": "2.3.2",
  "description": "Automate with simplicity",
  "license": "MIT",
  "homepage": "https://github.com/omarandstuff/desplega#readme",
  "repository": "github:omarandstuff/desplega",
  "bugs": "https://github.com/omarandstuff/desplega/issues",
  "author": "David De Anda <omarandstuff@gmail.com> (https://github.com/omarandstuff)",
  "scripts": {
    "clean": "rimraf dist coverage",
    "build": "babel src --out-dir dist",
    "test": "jest --watch",
    "test:coverage": "jest --coverage --no-cache",
    "playground": "babel-node playground",
    "format": "prettier --write \"{src,tests}/**/*.js\"",
    "format:check": "prettier --list-different \"{src,tests}/**/*.js\"",
    "prepare": "yarn clean && yarn format:check && yarn test:coverage && yarn build"
  },
  "dependencies": {
    "commander": "^2.19.0",
    "desplega-api": "^2.3.2",
    "js-yaml": "^3.12.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^23.6.0",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.2"
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/index.js",
  "preferGlobal": true,
  "bin": {
    "desplega": "dist/index.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "keywords": [
    "simple",
    "deployer",
    "desplega",
    "automate",
    "ssh",
    "pipeline"
  ]
}
