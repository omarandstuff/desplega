{
  "name": "simple-deployer",
  "version": "2.0.0",
  "description": "Automate with simplicity",
  "license": "MIT",
  "homepage": "https://github.com/omarandstuff/simple-deployer#readme",
  "repository": "github:omarandstuff/simple-deployer",
  "bugs": "https://github.com/omarandstuff/simple-deployer/issues",
  "keywords": [
    "simple",
    "deployer",
    "automate",
    "ssh"
  ],
  "author": "David De Anda <omarandstuff@gmail.com> (https://github.com/omarandstuff)",
  "main": "dist/simple.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "test": "jest --watch",
    "test:coverage": "jest --coverage --no-cache",
    "test:examples": "babel-node examples",
    "format": "prettier --write \"{src,test}/**/*.js\""
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "moment": "^2.22.2",
    "numeral": "^2.0.6",
    "ssh2": "^0.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^23.1.0",
    "prettier": "^1.13.5"
  },
  "npmName": "simple-deployer",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  }
}
