{
  "name": "desplega",
  "version": "2.0.0",
  "description": "Automate with simplicity",
  "license": "MIT",
  "homepage": "https://github.com/omarandstuff/desplega#readme",
  "repository": "github:omarandstuff/desplega",
  "bugs": "https://github.com/omarandstuff/desplega/issues",
  "keywords": [
    "simple",
    "deployer",
    "desplega",
    "automate",
    "ssh"
  ],
  "author": "David De Anda <omarandstuff@gmail.com> (https://github.com/omarandstuff)",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist coverage",
    "build": "babel src --out-dir dist",
    "test": "jest --watch",
    "test:coverage": "jest --coverage --no-cache",
    "test:examples": "babel-node examples",
    "test:playground": "babel-node playground",
    "format": "prettier --write \"{src,test}/**/*.js\"",
    "format:check": "prettier --list-different \"{src,test}/**/*.js\"",
    "prepare": "yarn clean && yarn format:check && yarn test:coverage && yarn build"
  },
  "dependencies": {
    "ansi-regex": "^3.0.0",
    "chalk": "^2.4.1",
    "moment": "^2.22.2",
    "numeral": "^2.0.6",
    "ssh2": "^0.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^23.1.0",
    "prettier": "^1.13.5",
    "rimraf": "^2.6.2"
  },
  "npmName": "desplega",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  }
}
